<?xml version="1.0" encoding="UTF-8"?>
  
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"   
   "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="biz">
<typeAlias alias="serviceCombo" type="com.maclaren.bank.biz.common.bean.ServiceCombo" />
	<select id="getServiceCombo" resultClass="serviceCombo">
		select 
			id service_id,
			serviceName
		from
			biz_service
	</select>
</sqlMap>
































